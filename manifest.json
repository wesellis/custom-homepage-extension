{
  "manifest_version": 3,
  "name": "Custom Home Page",
  "version": "1.0",
  "description": "A beautiful custom home page with time, Unsplash photos, and website shortcuts with screenshots",
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
  "https://api.unsplash.com/*",
  "https://images.unsplash.com/*",
  "https://api.microlink.io/*",
  "https://api.screenshotone.com/*",
      "https://www.google.com/*"
    ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'unsafe-eval'; object-src 'self';"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}